/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    And (a=a, b=b, out = andOut);
    Not (in = andOut, out = AandB);
    Not (in = a, out = notA);
    Not (in = b, out = notB);
    Nand (a = notA, b = notB, out = NotANotB);
    And (a = AandB, b = NotANotB, out = out);
}